#include<stdio.h>
 int main(){
 
float c1,c2,c3,prom;
printf("Ingresa tres calificaciones: ");
scanf("%f %f %f", &c1,&c2,&c3);
prom=(c1+c2+c3)/3;
if(prom>=9)
print("Excento");
else 
print("Ordinario");
//Fue solo que se ingresaran las calificaiones con esos datos se suman y se dividen entre 3 que es el numero de calificaciones si es mayor a 9 exenta   :)
 }
