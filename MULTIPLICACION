#include <stdio.h>
int main() {
    int a,b,c,r;
    printf("Tres numeros: ");
    scanf("%d %d %d", &a,&b,&c);
    r = a*b*c;
    printf("Resultado: %d", r);
    return 0;
// solo puse tres variables que son las que se tienen que ingresar e hice la operacion con * y le puse el resultado
}

