#include <stdio.h>
int main() {
    float base, alt, costo;
    printf("Base y altura: ");
    scanf("%f %f", &base, &alt);
    costo = base * alt * 1500;
    printf("Costo: %.2f", costo);
// solo se tenia que ingresar las dos variables que son la base altura se multiplican y luego por 1500 y ese es el resultado 
    return 0;

