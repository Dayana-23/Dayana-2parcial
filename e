#include<stdio.h>
 int main(){
float c1,c2,c3,prom;
printf("Ingresa tres calificaciones: ");
scanf("%f %f %f",&c1,&c2,&c3);
prom=(c1+c2+c3)/3;
if(prom>=9)
printf("exento");
else if(prom>6);
printf("Ordinario");
else 
printf("Extraordinario");
// pirmero es ingresar las calificaciones , se dividen entre tres y si es mayor a nueve exento y si es mayor a seis es ordinario

     
 }
