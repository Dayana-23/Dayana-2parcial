#include<stdio.h>
 int main(){
float c1,c2,c3,prom;
printf("Ingresa tres calificaciones: ");
scanf("%f %f %f",&c1,&c2,&c3);
prom=(c1+c2+c3)/3;
if(prom>=9)
printf("exento");
else if(prom>6);
printf("Ordinario");
else 
printf("Extraordinario");
\\ Fue casi lo mismo que el anterior solo agregue otros condicionantes

     
 }