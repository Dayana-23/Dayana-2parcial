#include <stdio.h>
int main() {
    int n, r;
    printf("Pon un numero: ");
    scanf("%d", &n);
    r = n * n;
    printf("El cuadrado es: %d", r);
    return 0;
// en este caso fue sólo buscar o hacer que ingresen el número y base eso, nada más, pues multiplicarlo por ese mismo numero que es como se saca el cuadrado
}

