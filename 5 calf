#include<stdio.h>
 int main(){
     float c1,c2,c3,c4,c5,p;
     printf("Ingresa cinco calificaciones: ");
     scanf("%f,%f,%f,%f,%f",&c1,&c2,&c3,&c4,&c5);
     p=(c1+c2+c3+c4+c5)/5;
     if(p>=6)
     printf("Aprovado");
     else 
     printf("Reprovado");
     //Lo mismo que los otros ejercicios solo agregue mas variables,hice las sumas y se divido entre 5 
     
 }
